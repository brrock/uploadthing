{
  "name": "@uploadthing/vue",
  "version": "6.3.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    "./styles.css": "./dist/style.css",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./index": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite build -w",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview"
  },
  "dependencies": {
    "@uploadthing/dropzone": "workspace:0.3.1",
    "@uploadthing/shared": "workspace:6.6.0",
    "@vueuse/core": "^10.9.0",
    "file-selector": "^0.6.0",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "autoprefixer": "10.4.14",
    "postcss": "8.4.38",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "uploadthing": "workspace:6.9.0",
    "vite": "^5.1.4",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  },
  "peerDependencies": {
    "uploadthing": "^6.9.0",
    "vue": "^3.4.0"
  }
}
