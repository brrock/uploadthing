{
  "name": "@uploadthing/expo",
  "version": "7.2.2",
  "type": "module",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint src --max-warnings 0",
    "build": "bunchee --tsconfig tsconfig.build.json",
    "clean": "git clean -xdf dist node_modules",
    "dev": "wait-on ../react/native/index.d.ts && bunchee -w --tsconfig tsconfig.build.json --no-clean",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@uploadthing/react": "workspace:*",
    "@uploadthing/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "18.3.3",
    "@uploadthing/eslint-config": "workspace:*",
    "@uploadthing/tsconfig": "workspace:*",
    "bunchee": "^6.2.0",
    "eslint": "9.17.0",
    "expo-constants": "^17.0.8",
    "expo-document-picker": "^13.0.3",
    "expo-image-picker": "^16.0.6",
    "react": "18.3.1",
    "react-native": "^0.76.7",
    "typescript": "5.7.2",
    "uploadthing": "workspace:*",
    "wait-on": "^8.0.1"
  },
  "peerDependencies": {
    "expo-constants": "*",
    "expo-document-picker": "*",
    "expo-image-picker": "*",
    "react": "^17.0.2 || ^18.0.0 || ^19.0.0",
    "react-native": "*",
    "uploadthing": "^7.2.0"
  },
  "peerDependenciesMeta": {
    "@uploadthing/mime-types": {
      "optional": true
    }
  }
}
